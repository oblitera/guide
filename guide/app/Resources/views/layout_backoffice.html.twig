<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        <script type="text/javascript" src="{{ asset('js/jquery-1.11.2.min.js') }}"> </script>
        <script type="text/javascript" src="{{ asset('js/bootstrap.min.js') }}"> </script>
        <link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet" type="text/css"> 
        <link href="{{ asset('css/style_admin.css') }}" rel="stylesheet" media="all" type="text/css">
        {% block cssSupplementaire %}{% endblock %}
        {% block jsSupplementaire %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}
    	<div class="row" id="back_office_menu">
			<div class="hidden-xs col-sm-2 col-md-2 col-lg-2"></div>
			<div class="col-xs-12 col-sm-8 col-md-8 col-lg-8">
				<ul class="nav navbar-nav hidden-xs" id="body_menu_d">
				    <li class="active"><a href="{{ path('gui_annonces_homepage') }}">Accueil</a></li>
				    <li><a href="#">Aide</a></li>
				    <li><a href="#">Contact</a></li>
				</ul>
			</div>
		</div>
		<div class="row" id="body_content">
			<div class="col-xs-12 col-sm-12 col-md-10 col-lg-8 col-md-offset-1 col-lg-offset-2">
				{% block body_content %}
				<div class="row">
					<div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
						{% block body_content_c_g %}
						<div class="block_type">
							<div class="bandeau-wrapper small_bandeau">
								<div class="bandeau-front">Votre profil</div>
								<div class="bandeau-edge-topleft"></div>
								<div class="bandeau-edge-topright"></div>
								<div class="bandeau-edge-bottomleft"></div>
								<div class="bandeau-edge-bottomright"></div>
								<div class="bandeau-back-left"></div>
								<div class="bandeau-back-right"></div>
							</div>
							<div class="row">
							</div>				
						</div>
						<div class="block_type">
							<div class="bandeau-wrapper small_bandeau">
								<div class="bandeau-front">Vos services</div>
								<div class="bandeau-edge-topleft"></div>
								<div class="bandeau-edge-topright"></div>
								<div class="bandeau-edge-bottomleft"></div>
								<div class="bandeau-edge-bottomright"></div>
								<div class="bandeau-back-left"></div>
								<div class="bandeau-back-right"></div>
							</div>
							<div class="row">
							</div>				
						</div>
						{% endblock %}
					</div>
					<div class="col-xs-8 col-sm-8 col-md-8 col-lg-8 col-xs-offset-1 col-sm-offset-1 col-md-offset-1 col-lg-offset-1">
						{% block body_content_c_d %}
						{% endblock %}
					</div>
				</div>
				{% endblock %}			
			</div>
		</div>
		<div class="row" id="body_footer">
		</div>
        {% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>